{
  "name": "@whitecloak/nestjs-auth0-jwt",
  "version": "0.1.1",
  "description": "Auth0 JWT Module for NestJS Framework",
  "author": "White Cloak Technologies, Inc.",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "dependencies": {
    "@nestjs/common": "^7.6.7",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/passport": "^7.1.5",
    "apollo-server-express": "^2.19.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "jwks-rsa": "^1.12.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "ts-jest": "^26.4.4"
  },
  "devDependencies": {
    "@nestjs/core": "^7.6.7",
    "@nestjs/platform-express": "^7.6.7",
    "@nestjs/testing": "^7.6.7",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
